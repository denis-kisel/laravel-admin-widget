{
    "name": "denis-kisel/laravel-admin-widget",
    "description": "Widget for laravel admin",
    "authors": [
        {
            "name": "Denis Kisel",
            "email": "denis.kisel92@gmail.com",
            "homepage": "http://digital-elephant.com.ua"
        }
    ],
    "homepage": "https://github.com/denis-kisel/laravel-admin-widget",
    "keywords": ["Laravel", "LaravelAdminWidget"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "denis-kisel/summernote": "*",
        "denis-kisel/repeator": "*"

    },
    "autoload": {
        "psr-4": {
            "DenisKisel\\LaravelAdminWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DenisKisel\\LaravelAdminWidget\\LaravelAdminWidgetServiceProvider"
            ],
            "aliases": {
                "LaravelAdminWidget": "DenisKisel\\LaravelAdminWidget\\Facades\\LaravelAdminWidget"
            }
        }
    }
}
